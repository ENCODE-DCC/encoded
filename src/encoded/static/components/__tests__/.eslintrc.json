{
    "extends": "airbnb",
    "env": {
        "jest": true
    },
    "rules": {
        "comma-dangle": ["error", {
            "arrays": "always-multiline",
            "objects": "always-multiline"
        }],
        "global-require": 0,
        "indent": [
            2,
            4
        ],
        "max-len": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0
    }
}
