{
    "title": "Patient",
    "description": "Schema for submitting patient demographics.",
    "id": "/profiles/patient.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "identifyingProperties": ["uuid", "accession", "aliases"],
    "required": ["sex", "race", "ethnicity"],
    "additionalProperties": false,
    "mixinProperties": [
        { "$ref": "mixins.json#/schema_version" },
        { "$ref": "mixins.json#/accession" },
        { "$ref": "mixins.json#/accessioned_status" },
        { "$ref": "mixins.json#/submitted" },
        { "$ref": "mixins.json#/aliases" },
        { "$ref": "mixins.json#/notes" },
        { "$ref": "mixins.json#/uuid" }
    ],
    "type": "object",
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "accession": {
            "accessionType": "PT"
        },
        "sex": {
            "title": "Sex",
            "type": "string",
            "enum": [
                "Female",
                "Male",
                "Unknown"
            ]
        },

        "race": {
            "title": "Race",
            "type": "string",
            "enum": [
                "American Indian",
                "Asian",
                "Black",
                "Hawaiian Pacific Islander",
                "White",
                "Declined",
                "Other"
            ]
        },
        "ethnicity": {
            "title": "Ethnicity",
            "type": "string",
            "enum": [
                "Hispanic",
                "Non-hispanic",
                "Declined",
                "Unknown"
            ]
        },
        "mrn": {
            "title": "Medical Number",
            "comment": "KCE personnel are allowed to set this value.",
            "type": "string",
            "permission": "import_items",
            "uniqueKey": true
        },

        "death_date": {
            "title": "Death Date",
            "description": "Death date of patient.",
            "comment": "Date can be submitted as YYYY-MM-DD.",
            "type": "string",
            "anyOf": [
                {
                    "format": "date"
                }
            ]
        },
        "death_source": {
            "title": "Death Source",
            "description": "The source of the death date",
            "type": "string",
            "enum": [
                "UTSouthwestern Hospital",
                "Parkland Hospital",
                "Tumor Registry"
            ]
        }

    },
    "facets": {
        "diagnosis.age_range": {
            "title": "Age at Diagnosis"
        },
        "sex": {
            "title": "Sex"
        },
        "ethnicity": {
            "title": "Ethnicity"
        },
        "race": {
            "title": "Race"
        },
        "vital_status": {
            "title": "Vital Status"
        },

        "medical_imaging.type" : {
          "title": "Medical Imaging"
        },

        "surgery_summary": {
            "title": "Surgery Treatment Status"
        },
        "surgery.surgery_procedure.surgery_type": {
            "title": "Surgery Procedure"
        },
        "surgery.hospital_location": {
            "title": "Surgery Hospital Location"
        },
        "surgery.surgery_procedure.nephrectomy_details.type": {
            "title": "Nephrectomy Type"
        },
        "surgery.surgery_procedure.nephrectomy_details.approach": {
            "title": "Nephrectomy Approach"
        },

        "sur_nephr_robotic_assist":{
            "title": "Nephrectomy Robotic Assist"
        },
        "sur_path_tumor_size":{
            "title": "Tumor Size"

        },
        "surgery.pathology_report.laterality": {
            "title": "Tumor Laterality"
        },

        "surgery.pathology_report.histology": {
            "title": "Tumor Histologic Subtypes"
        },
        "surgery.pathology_report.ajcc_p_stage": {
            "title": "pT Stage"
        },
        "surgery.pathology_report.n_stage": {
            "title": "pN Stage"
        },
        "surgery.pathology_report.m_stage": {
            "title": "pM Stage"
        },
        "surgery.pathology_report.metasis_details.site": {
            "title": "Metasis Site"
        },
        "surgery.pathology_report.metasis_details.type": {
            "title": "Metasis Type"
        },
        "surgery.pathology_report.ajcc_tnm_stage": {
            "title": " TNM Stage"
        },
        "ihc.antibody": {
            "title": "IHC/FISH (Clinical lab)"
        },
        "ihc.result": {
            "title": "IHC/FISH results"
        },
        "medications.name":{
            "title":"Medication Name"
        },
        "medication_range":{
            "title":"Medication Duration"
        },
        "germline_summary": {
            "title": "Germline Mutation"
        },
        "radiation_summary": {
            "title": "Radiation Treatment Status"
        },
        "radiation.dose_range": {
            "title": "Radiation Dose per Fraction"
        },
        "radiation.fractions_range": {
            "title": "Radiation Fractions"
        },
        "radiation.site_general" : {
          "title": "Radiation Site"
        },
        "radiation.treatment_intention" : {
          "title": "Radiation Treatment Intention"
        },
        "metastasis_status": {
            "title": "Metastasis Status"
        },
        "metastasis.site": {
            "title": "Metastatic Site"
        },
        "metastasis.source": {
            "title": "Metastasis Source"
        },

        "biospecimen.sample_type": {
              "title": "Biospecimen Sample Type"
        },
        "biospecimen.tissue_derivatives": {
            "title": "Biospecimen Tissue Derivatives"
        },
        "biospecimen.tissue_type": {
            "title": "Biospecimen Tissue Type"
        },
        "biospecimen.anatomic_site": {
            "title": "Biospecimen Anatomic Site"
        },
        "biospecimen.distributed": {
            "title": "Biospecimen Distribution Status"
        },

        "status": {
            "title": "Status"
        }
    },
    "columns": {
        "accession": {
            "title": "Accession"
        },
        "diagnosis.age": {
            "title": "Diagnosis age"
        },
        "diagnosis.age_unit": {
            "title": "Diagnosis age units"
        },
        "sex": {
            "title": "Sex"
        },
        "ethnicity": {
            "title": "Ethnicity"
        },
        "race": {
            "title": "Race"
        },
        "status": {
            "title": "Status"
        },
        "sur_nephr_robotic_assist":{
            "title": "Nephrectomy Robotic Assist"
        },
        "sur_path_tumor_size":{
            "title": "Tumor Size"

        },
        "surgery.pathology_report.laterality": {
            "title": "Tumor Laterality"
        },

        "surgery.pathology_report.histology": {
            "title": "Tumor Histologic Subtypes"
        },
        "surgery.pathology_report.ajcc_p_stage": {
            "title": "pT Stage"
        },
        "surgery.pathology_report.n_stage": {
            "title": "pN Stage"
        },
        "surgery.pathology_report.m_stage": {
            "title": "pM Stage"
        },
        "surgery.pathology_report.metasis_details.site": {
            "title": "Metasis Site"
        },
        "surgery.pathology_report.metasis_details.type": {
            "title": "Metasis Type"
        },
        "surgery.pathology_report.ajcc_tnm_stage": {
            "title": " AJCC TNM Stage"
