{
    "title": "scATAC-seq multiplet quality metric",
    "description": "Schema for scATAC-seq multiplet quality metrics",
    "id": "/profiles/sc_atac_multiplet_quality_metric.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": ["step_run","quality_metric_of", "award", "lab"],
    "additionalProperties": false,
    "identifyingProperties": ["uuid"],
    "mixinProperties": [
        { "$ref": "mixins.json#/schema_version" },
        { "$ref": "quality_metric.json#/properties" },
        { "$ref": "mixins.json#/uuid" },
        { "$ref": "mixins.json#/submitted"},
        { "$ref": "mixins.json#/aliases"},
        { "$ref": "mixins.json#/attribution" },
        { "$ref": "mixins.json#/standard_status"},
        { "$ref": "mixins.json#/assay" }
    ],
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "original_barcode_count": {
            "title": "Number of barcodes before multiplet filtering",
            "type": "integer",
            "minimum": 0
        },
        "final_barcode_count": {
            "title": "Final number of barcodes",
            "description": "Number of barcodes after excluding non-primary multiplet barcodes",
            "type": "integer",
            "minimum": 0
        },
        "analyzed_barcode_count": {
            "title": "Number of barcodes analyzed",
            "description": "Number of barcodes analyzed (min 500 fragments)",
            "type": "integer",
            "minimum": 0
        },
        "multiplet_barcode_count": {
            "title": "Number of multiplets barcodes",
            "description": "Number of barcodes belonging to multiplets",
            "type": "integer",
            "minimum": 0
        },
        "frac_multiplets": {
            "title": "Fraction of multiplet barcodes",
            "description": "Fraction of barcodes belonging to multiplets",
            "type": "number",
            "minimum": 0,
            "maximum": 1
        },
        "multiplet_threshold": {
            "title": "Multiplet threshold",
            "description": "Estimated pairwise Jaccard distance threshold for multiplet detection",
            "type": "number",
            "minimum": 0,
            "maximum": 1
        },
        "multiplet_stats": {
            "title": "Multiplet detection statistics file",
            "type": "object",
            "additionalProperties": false,
            "attachment": true,
            "formInput": "file",
            "properties": {
                "download": {
                    "title": "File Name",
                    "type": "string"
                },
                "href": {
                    "comment": "Internal webapp URL for document file",
                    "type": "string"
                },
                "type": {
                    "title": "MIME type",
                    "type": "string",
                    "enum": [
                        "text/plain"
                    ]
                },
                "size": {
                    "title": "File size",
                    "type": "integer",
                    "minimum": 0
                },
                "md5sum": {
                    "format": "md5sum",
                    "type": "string",
                    "title": "MD5sum"
                }
            }
        },
        "barcodes_status": {
            "title": "Per-barcode multiplet status",
            "type": "object",
            "additionalProperties": false,
            "attachment": true,
            "formInput": "file",
            "properties": {
                "download": {
                    "title": "File Name",
                    "type": "string"
                },
                "href": {
                    "comment": "Internal webapp URL for document file",
                    "type": "string"
                },
                "type": {
                    "title": "MIME type",
                    "type": "string",
                    "enum": [
                        "text/tab-separated-values"
                    ]
                },
                "size": {
                    "title": "File size",
                    "type": "integer",
                    "minimum": 0
                },
                "md5sum": {
                    "format": "md5sum",
                    "type": "string",
                    "title": "MD5sum"
                }
            }
        },
        "barcode_pairs_multiplets": {
            "title": "Multiplet barcode pair statistics",
            "type": "object",
            "additionalProperties": false,
            "attachment": true,
            "formInput": "file",
            "properties": {
                "download": {
                    "title": "File Name",
                    "type": "string"
                },
                "href": {
                    "comment": "Internal webapp URL for document file",
                    "type": "string"
                },
                "type": {
                    "title": "MIME type",
                    "type": "string",
                    "enum": [
                        "text/tab-separated-values"
                    ]
                },
                "size": {
                    "title": "File size",
                    "type": "integer",
                    "minimum": 0
                },
                "md5sum": {
                    "format": "md5sum",
                    "type": "string",
                    "title": "MD5sum"
                }
            }
        },
        "barcode_pairs_expanded": {
            "title": "Total barcode pair statistics",
            "type": "object",
            "additionalProperties": false,
            "attachment": true,
            "formInput": "file",
            "properties": {
                "download": {
                    "title": "File Name",
                    "type": "string"
                },
                "href": {
                    "comment": "Internal webapp URL for document file",
                    "type": "string"
                },
                "type": {
                    "title": "MIME type",
                    "type": "string",
                    "enum": [
                        "text/tab-separated-values",
                        "application/gzip",
                        "application/x-gzip"
                    ]
                },
                "size": {
                    "title": "File size",
                    "type": "integer",
                    "minimum": 0
                },
                "md5sum": {
                    "format": "md5sum",
                    "type": "string",
                    "title": "MD5sum"
                }
            }
        },
        "multiplet_threshold_plot": {
            "title": "Plot illustrating multiplet Jaccard distance threshold",
            "type": "object",
            "additionalProperties": false,
            "attachment": true,
            "formInput": "file",
            "properties": {
                "download": {
                    "title": "File Name",
                    "type": "string"
                },
                "href": {
                    "comment": "Internal webapp URL for document file",
                    "type": "string"
                },
                "type": {
                    "title": "MIME type",
                    "type": "string",
                    "enum": [
                        "image/png"
                     ]
                },
                "size": {
                    "title": "File size",
                    "type": "integer",
                    "minimum": 0
                },
                "md5sum": {
                    "format": "md5sum",
                    "type": "string",
                    "title": "MD5sum"
                },
                "width": {
                    "title": "Image width",
                    "type": "integer",
                    "minimum": 0
                },
                "height": {
                    "title": "Image height",
                    "type": "integer",
                    "minimum": 0
                }
            }
        }
    },
    "facets": {
        "assay_term_name": {
            "title": "Assay name"
        },
        "status": {
            "title": "Status"
        }
    },
    "changelog": "/profiles/changelogs/sc_atac_multiplet_quality_metric.md"
}
