{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "func-names": 0,
        "indent": [
            2,
            4
        ],
        "max-len": 0,
        "no-console": 0,
        "no-nested-ternary": 0,
        "no-param-reassign": [
            2,
            {
                "props": false
            }
        ],
        "object-shorthand": 0,
        "react/forbid-prop-types": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "react/no-deprecated": 0,
        "react/no-multi-comp": 0,
        "react/prefer-es6-class": 0,
        "react/prefer-stateless-function": 0
    }
}